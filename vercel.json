{
  "version": 2,
  "headers": [
    {
      "source": "/console/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, s-maxage=3600, max-age=3600"
        }
      ]
    }
  ],
  "rewrites": [
    {"source": "/dsm/(.*)", "destination": "https://www.yutangli.net/dsm/$1"},
    {"source": "/(.*)", "destination": "https://www.baidu.com/$1"}
  ]
}
